<%= "Conflicts: #{@students.length - @recitation_assignments.length}" %>
<% @reverted_matching.each do |k, v| %>
  <%= "\nSection: #{k} (#{v.length} students)" %>
  <% v.map do |a| %>
    <%= "#{a}@mit.edu" %>
  <% end %>
<% end %>

<%= "\nUnassigned students:" %>
<% (@students.map { |s| s[:athena] } - @recitation_assignments.keys).map do |a| %>
  <%= "#{a}@mit.edu" %>
<% end %>
