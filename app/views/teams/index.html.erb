<h1>Listing teams</h1>

<table>
  <tr>
    <th>Team set</th>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @teams.each do |team| %>
  <tr>
    <td><%= team.team_set_id %></td>
    <td><%= team.name %></td>
    <td><%= link_to 'Show', team %></td>
    <td><%= link_to 'Edit', edit_team_path(team) %></td>
    <td>
      <%= link_to 'Destroy', team, data: { confirm: 'Are you sure?' },
            method: :delete %>
    </td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New team', new_team_path %>
