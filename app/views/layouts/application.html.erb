<!DOCTYPE html>
<html>
<head>
  <title>MIT <%= Course.main.number %></title>
<%= csrf_meta_tags %>
<%= favicon_link_tag %>
<%= stylesheet_link_tag "application" %>
<%= javascript_include_tag "application" %>
<%= render 'layouts/google_analytics' %>
</head>
<body class="user">
<header>
<%= render 'layouts/header' %>
</header>

<div id="real-body">
<%= render 'layouts/status_bar' %>
<div id="content-wrapper">
<article id="sidebar">
<%= render 'sessions/sidebar' %>
</article>
<article id="main">
<%= yield %>
</article>
</div>
</div>

<footer>
<%= render 'layouts/footer' %>
</footer>
</body>
</html>
