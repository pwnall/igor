<% form_for(@feedback_answer) do |f| %>
  <%= f.error_messages %>

  <div class="field">
    <%= f.label :assignment_feedback_id %><br />
    <%= f.text_field :assignment_feedback_id %>
  </div>
  <div class="field">
    <%= f.label :question_id %><br />
    <%= f.text_field :question_id %>
  </div>
  <div class="field">
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </div>
  <div class="field">
    <%= f.label :target_user_id %><br />
    <%= f.text_field :target_user_id %>
  </div>
  <div class="field">
    <%= f.label :number %><br />
    <%= f.text_field :number %>
  </div>
  <div class="field">
    <%= f.label :comment %><br />
    <%= f.text_field :comment %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
